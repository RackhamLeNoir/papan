  "version": "0.0.1",
  "description": "A turn-by-turn board game engine",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "start-headless": "node .",
    "lint": "standard | snazzy",
    "node_test": "mocha ./test/common",
    "electron_test": "electron-mocha ./test/common",
    "test": "npm run node_test && npm run electron_test && npm run lint",
    "postinstall": "bower install && bower prune",
    "build": "electron-packager . --all --out=out"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
